<Window
    x:Class="TEC_App.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:candidateParts="clr-namespace:TEC_App.Parts.CandidateParts"
    xmlns:courseParts="clr-namespace:TEC_App.Parts.CourseParts"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TEC_App"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:parts="clr-namespace:TEC_App.Parts"
    Title="MainWindow"
    Width="1000"
    Height="450"
    Loaded="MainWindow_OnLoaded"
    mc:Ignorable="d">
    <TabControl Margin="5" TabStripPlacement="Left">
        <TabItem Header="Candidate">
            <Grid>
                <candidateParts:CandidateUC />
            </Grid>
        </TabItem>
        <TabItem Header="Opening">
            <Grid>
                <parts:OpeningUC />
            </Grid>
        </TabItem>
        <TabItem Header="Course">
            <Grid>
                <courseParts:CourseUC />
            </Grid>
        </TabItem>

    </TabControl>

</Window>
